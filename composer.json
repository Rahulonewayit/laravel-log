{
    "name": "Rahulonewayit/laravel-log",
    "description": "Simple API to write logs for Laravel.",
    "keywords": ["log","laravel","Monolog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Marcos",
            "email": "edujugon@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.1",
        "illuminate/log": "^5.1"
    },
    "require-dev": {
      "phpunit/phpunit": "~5.0"
    },
    "autoload": {
      "psr-4": {
        "Rahulonewayit\\Log\\": "src/"
      },
      "files": [
            "src/Support/helpers.php"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Rahulonewayit\\Log\\Providers\\LogServiceProvider"
        ],
        "aliases": {
          "PushNotification": "Rahulonewayit\\Log\\Facades\\Log"
        }
      }
    },
    "minimum-stability": "dev"
}
